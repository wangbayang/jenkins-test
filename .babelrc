{
  "presets": [
    [
      "env",
      {
        "target": {
          "browsers": ["last 2 versions", "ie >= 10"]
        }
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ]
}